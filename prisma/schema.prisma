generator client {
  provider = "prisma-client-js"
  // Hapus baris output, biarkan default
}

datasource db {
  provider = "postgresql" // DULUNYA "sqlite", GANTI JADI INI
  url      = env("DATABASE_URL")
}

model Note {
  id        Int      @id @default(autoincrement())
  content   String
  createdAt DateTime @default(now())
}